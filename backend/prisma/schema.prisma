
generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model Collaborator {
    id     Int     @id @unique @default(autoincrement())
    name   String  @db.VarChar(50)
    daily  Int     
    img  String    @db.VarChar(255)
    createdAt  DateTime   @default(now())
    updatedAt  DateTime   @default(now())
    @@map("collaborators")
}